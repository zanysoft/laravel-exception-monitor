{
  "name": "zanysoft/laravel-exception-monitor",
  "description": "Little Laravel package to notify you about exceptions in your application.",
  "keywords": [
    "laravel",
    "laravel exception",
    "exception monitor",
    "zanysoft"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Zany Soft",
      "email": "info@zanysoft.net"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "^6.0 |^7.0|^8.0|^9.0|^10.0",
  },
  "autoload": {
    "psr-4": {
      "ZanySoft\\LaravelExceptionMonitor\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZanySoft\\LaravelExceptionMonitor\\ExceptionMonitorServiceProvider"
      ],
      "aliases": {
        "ExceptionMonitor": "ZanySoft\\LaravelExceptionMonitor\\ExceptionMonitor"
      }
    }
  },
  "minimum-stability": "stable"
}
